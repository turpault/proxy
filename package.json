{
  "name": "proxy",
  "version": "1.0.0",
  "description": "A Bun reverse proxy server with Let's Encrypt certificate generation and process management",
  "scripts": {
    "start": "bun --watch --inspect=localhost:6499/ src/index.ts",
    "start:dev": "NODE_ENV=development bun --watch --inspect=localhost:6499/ src/index.ts",
    "test": "bun test",
    "lint": "eslint src/**/*.ts"
  },
  "engines": {
    "bun": ">=1.1.30"
  },
  "keywords": [
    "nginx",
    "reverse-proxy",
    "letsencrypt",
    "ssl",
    "typescript",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {

    "@types/cron": "^2.4.3",
    "@types/geoip-lite": "^1.4.4",
    "@types/mime-types": "^3.0.1",
    "acme-client": "^5.4.0",

    "chalk": "^5.4.1",
    "cron": "^4.3.1",
    "dotenv": "^17.1.0",
    "fs-extra": "^11.3.0",
    "geoip-lite": "^1.4.10",
    "joi": "^17.13.3",
    "mime-types": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "winston": "^3.17.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9.30.1"
  },
  "module": "src/index.ts",
  "type": "module",
  "private": true
}